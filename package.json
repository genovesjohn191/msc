{
  "name": "fusion",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl --ssl-key ssl/dev-portal.key --ssl-cert ssl/dev-portal.crt",
    "build": "node --max_old_space_size=3000 ./node_modules/@angular/cli/bin/ng build",
    "clean": "rm -rf node_modules",
    "reinstall": "yarn run clean && yarn install",
    "rebuild": "yarn run reinstall && yarn run build",
    "test": "ng test --browsers=ChromeHeadless --code-coverage --watch=false",
    "test:watch": "ng test --browsers=ChromeHeadless --code-coverage --watch=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "output:styling": "gulp",
    "build:prod": "node --max_old_space_size=3000 ./node_modules/@angular/cli/bin/ng build --configuration=production && yarn run output:styling"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^4.0.5",
